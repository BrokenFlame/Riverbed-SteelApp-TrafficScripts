$poolName = "HTTP - Moonpig.com";
if (pool.activeNodes ($poolName) <= 0)
{
   http.stream.startResponse("503", "text/html", "", "Server: NLB");
   $httpResponseBody = "<!DOCTYPE html><html><head><title>The Moonpig Website is Currently undergoing maintenance - Please try again later.</title><style>.message{background:url(data:image/png;base64,);background-repeat:no-repeat;width:798px;padding-top:350px;text-align:center;margin: 0 auto;font-family:Arial, Sans-Serif;color:#33318E;}</style></head><body><div class=\"message\"><h1>The Moonpig website is currently undergoing maintenance.</h1><p>We should be back in a few moments.<br /></p><img src=\"data:image/gif;base64,\"/></div></body></html>";
   http.stream.writeResponse($httpResponseBody);
   http.stream.finishResponse();
}
